    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "fs": "^0.0.1-security",
    "gif-countdown": "^0.4.0",
    "http-errors": "^2.0.0",
    "node-gzip": "^1.1.2",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-http": "^3.2.0",
    "serverless-plugin-common-excludes": "^4.0.0",
